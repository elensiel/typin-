[sub_resource type="GDScript" id="GDScript_iwgq4"]
resource_name = "restart_key_buttons"
script/source = "extends VBoxContainer

func _ready() -> void:
	var buttons : Array[Button]
	
	for button in $HBoxContainer.get_children():
		buttons.append(button)
	owner.restart_buttons = buttons
	
	for button in buttons:
		button.toggled.connect(Callable.create(self, \"_on_toggled\").bind(button))

func _on_toggled(toggled_on: bool, emitting_button: Button) -> void:
	if toggled_on:
		for button in owner.restart_buttons:
			if button.button_pressed and button != emitting_button:
				button.button_pressed = false
		
		if emitting_button.name.match(\"Off\"):
			SettingsManager.changed_settings.general.quick_restart_off = true
		else:
			SettingsManager.changed_settings.general.quick_restart_off = false
			match emitting_button.name:
				\"Esc\":
					SettingsManager.changed_settings.general.quick_restart = InputManager.ESC
				\"Tab\":
					SettingsManager.changed_settings.general.quick_restart = InputManager.TAB
				\"Enter\":
					SettingsManager.changed_settings.general.quick_restart = InputManager.ENTER
"

