[sub_resource type="GDScript" id="GDScript_vlppf"]
resource_name = "window_mode"
script/source = "extends OptionButton

func _enter_tree() -> void: owner.window_mode = self

func _on_item_selected(index: int) -> void:
	match index:
		0:
			SettingsManager.changed_settings.general.window_mode = DisplayServer.WINDOW_MODE_WINDOWED
		1:
			SettingsManager.changed_settings.general.window_mode = DisplayServer.WINDOW_MODE_MAXIMIZED
		2:
			SettingsManager.changed_settings.general.window_mode = DisplayServer.WINDOW_MODE_FULLSCREEN
"

