[sub_resource type="GDScript" id="GDScript_wr3al"]
resource_name = "button_tabs"
script/source = "extends HBoxContainer

const OUTLINE_SIZE: int = 20

var tab_buttons : Array[Button]
var tab_contents : Array[MarginContainer]

func _enter_tree() -> void:
	for button in get_children():
		tab_buttons.append(button)
	
	for button in tab_buttons:
		button.toggled.connect(Callable.create(self, &\"_on_toggled\").bind(button))

func _ready() -> void:
	tab_buttons[0].button_pressed = true

func _on_toggled(toggled_on: bool, emitting_button: Button) -> void:
	if toggled_on:
		for i in tab_buttons.size():
			var tab_button: Button = tab_buttons[i]
			var tab_content: MarginContainer = tab_contents[i]
			
			if tab_button.button_pressed and tab_button != emitting_button:
				tab_button.button_pressed = false
			
			tab_content.visible = tab_button.button_pressed
			_update_outline(tab_button.button_pressed, tab_button)

func _update_outline(enabled: bool, button: Button) -> void:
	if enabled: 
		return button.add_theme_constant_override(&\"outline_size\", OUTLINE_SIZE)
	button.remove_theme_constant_override(&\"outline_size\")
"

[sub_resource type="GDScript" id="GDScript_iwgq4"]
resource_name = "window_mode"
script/source = "extends OptionButton

var current_window_mode: int = SettingsManager.current_settings.general.window_mode

func update_selection() -> void:
	for item in item_count:
		var window_mode: int = get_item_id(item)
		if current_window_mode == window_mode:
			current_window_mode = window_mode
			selected = item
			break

func restore_default() -> void:
	if current_window_mode != SettingsManager.DEFAULT_SETTINGS.general.window_mode:
		@warning_ignore(\"int_as_enum_without_cast\")
		@warning_ignore(\"int_as_enum_without_match\")
		DisplayServer.window_set_mode(SettingsManager.DEFAULT_SETTINGS.general.window_mode)
		current_window_mode = DisplayServer.window_get_mode()
		#SettingsManager.current_settings.general.window_mode = current_window_mode
		
		%WindowResizable.update_selection() # update resizable disabled flag

func _on_item_selected(_index: int) -> void: 
	var id := get_selected_id()
	
	if current_window_mode != id:
		DisplayServer.window_set_mode(id)
		current_window_mode = DisplayServer.window_get_mode()
		SettingsManager.current_settings.general.window_mode = current_window_mode
		
		%WindowResizable.update_selection() # update resizable disabled flag
"

[sub_resource type="GDScript" id="GDScript_0bj2m"]
resource_name = "resizable"
script/source = "extends HBoxContainer

var buttons : Array[Button]

func _enter_tree() -> void:
	buttons = [$Yes, $No]
	
	for button in buttons:
		button.toggled.connect(Callable.create(self, &\"_on_toggled\").bind(button))

func update_selection() -> void:
	var current_window_mode: int = DisplayServer.window_get_mode()
	
	buttons[0].disabled = current_window_mode == DisplayServer.WindowMode.WINDOW_MODE_FULLSCREEN
	buttons[1].disabled = current_window_mode == DisplayServer.WindowMode.WINDOW_MODE_FULLSCREEN
	
	buttons[0].button_pressed = SettingsManager.current_settings.general.resizable
	buttons[1].button_pressed = !SettingsManager.current_settings.general.resizable

func restore_default() -> void:
	var is_disabled: bool = SettingsManager.DEFAULT_SETTINGS.general.window_mode == DisplayServer.WindowMode.WINDOW_MODE_FULLSCREEN
	
	buttons[0].disabled = is_disabled
	buttons[1].disabled = is_disabled
	
	buttons[0].button_pressed = SettingsManager.DEFAULT_SETTINGS.general.resizable
	buttons[1].button_pressed = !SettingsManager.DEFAULT_SETTINGS.general.resizable

func _on_toggled(toggled_on: bool, emitting_button: Button) -> void:
	if toggled_on:
		for button in buttons:
			if button.button_pressed and button != emitting_button:
				button.button_pressed = false
		
		SettingsManager.current_settings.general.resizable = emitting_button.name.match(&\"Yes\")
		get_viewport().get_window().unresizable = !SettingsManager.current_settings.general.resizable
"

[sub_resource type="GDScript" id="GDScript_t382g"]
resource_name = "quick_restart"
script/source = "extends VBoxContainer

var buttons : Array[Button]

func _enter_tree() -> void:
	buttons = [
		$HBoxContainer/Off, 
		$HBoxContainer/Esc, 
		$HBoxContainer/Tab, 
		$HBoxContainer/Enter
	]
	
	for button in buttons:
		button.toggled.connect(Callable.create(self, &\"_on_toggled\").bind(button))

func update_selection() -> void:
	buttons[0].button_pressed = !SettingsManager.current_settings.general.quick_restart_active
	if !buttons[0].button_pressed:
		for button in buttons:
			if button.name == SettingsManager.current_settings.general.quick_restart:
				button.button_pressed = true
				break

func restore_default() -> void:
	if !buttons[0].button_pressed:
		buttons[0].button_pressed = true

func _on_toggled(toggled_on: bool, emitting_button: Button) -> void:
	if toggled_on:
		for button in buttons:
			if button.button_pressed and button != emitting_button:
				button.button_pressed = false 
		
		InputManager.quick_restart_key_active = !emitting_button.name.match(&\"Off\")
		SettingsManager.current_settings.general.quick_restart_active = InputManager.quick_restart_key_active
		
		if InputManager.quick_restart_key_active:
			InputManager.quick_restart_key = emitting_button.name
			SettingsManager.current_settings.general.quick_restart = InputManager.quick_restart_key
"

