[sub_resource type="GDScript" id="GDScript_xk1fi"]
resource_name = "font_size"
script/source = "extends OptionButton

const INTERVAL: int = 4
const MINIMUM_FONT_SIZE: int = 30
const MAXIMUM_FONT_SIZE: int = 82

func _enter_tree() -> void:
	for i in range(MINIMUM_FONT_SIZE, MAXIMUM_FONT_SIZE, INTERVAL):
		add_item(str(i))

func update_selection() -> void:
	selected = (SettingsManager.current_settings.appearance.font_size - MINIMUM_FONT_SIZE) / INTERVAL

func restore_default() -> void:
	var current: int = SettingsManager.current_settings.appearance.font_size
	var default: int = SettingsManager.DEFAULT_SETTINGS.appearance.font_size
	
	if current != default: ThemeManager.set_font_size(default)

func _on_item_selected(index: int) -> void:
	var current: int = SettingsManager.current_settings.appearance.font_size
	var new: int = MINIMUM_FONT_SIZE + (INTERVAL  * index)
	
	if current != new: ThemeManager.set_font_size(new)
"

[sub_resource type="GDScript" id="GDScript_urpkm"]
resource_name = "visible_lines"
script/source = "extends OptionButton

const MAXIMUM_LINES: int = 6

func _enter_tree() -> void:
	for i in range(1, MAXIMUM_LINES + 1):
		add_item(str(i))

func update_selection() -> void:
	selected = SettingsManager.current_settings.appearance.visible_lines - 1

func restore_default() -> void:
	var current: int = SettingsManager.current_settings.appearance.visible_lines
	var default: int = SettingsManager.DEFAULT_SETTINGS.appearance.visible_lines
	
	if current != default:
		SettingsManager.current_settings.appearance.visible_lines = default
		ObjectReferences.test_field_panel.queue_redraw()

func _on_item_selected(index: int) -> void:
	var current: int = SettingsManager.current_settings.appearance.visible_lines
	var new: int = index + 1
	
	if current != new:
		SettingsManager.current_settings.appearance.visible_lines = new
		ObjectReferences.test_field_panel.queue_redraw()
"

[sub_resource type="GDScript" id="GDScript_2ll5a"]
resource_name = "font"
script/source = "extends VBoxContainer

var font_buttons : Array[Button]

func _enter_tree() -> void:
	# appending every buttons
	for line in get_children():
		for button in line.get_children():
			font_buttons.append(button)
	
	# connecting every buttons
	for button in font_buttons:
		button.toggled.connect(Callable.create(self, &\"_on_toggled\").bind(button))

func update_selection() -> void:
	if SettingsManager.current_settings.appearance.font:
		for button in font_buttons:
			if SettingsManager.current_settings.appearance.font == button.get_theme_font(&\"font\"):
				button.button_pressed = true
				break

func restore_default() -> void:
	if !font_buttons[0].button_pressed:
		font_buttons[0].button_pressed = true

func _on_toggled(toggled_on: bool, emitting_button: Button) -> void:
	if toggled_on:
		for button in font_buttons:
			if button.button_pressed and button != emitting_button:
				button.button_pressed = false
		
		var current: Font = SettingsManager.current_settings.appearance.font
		var new: Font = emitting_button.get_theme_font(&\"font\")
		
		if current != new: ThemeManager.set_font(new)
"

