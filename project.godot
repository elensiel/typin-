;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="typin"
run/main_scene="res://scene/main.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
run/max_fps=60
run/low_processor_mode=true
config/icon="res://icon.svg"

[autoload]

StateMachine="*res://script/global/state_machine.gd"
TextManager="*res://script/global/text_manager.gd"
TypingManager="*res://script/global/typing_manager.gd"
ScoreManager="*res://script/global/score_manager.gd"
TimerManager="*res://script/global/timer_manager.gd"
InputManager="*res://script/global/input_manager.gd"
SettingsManager="*res://script/global/settings_manager.gd"
UiManager="*res://script/global/ui_manager.gd"
ThemeManager="*res://script/global/theme_manager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0

[global_group]

adjustable_displays="nodes with adjust_display()"

